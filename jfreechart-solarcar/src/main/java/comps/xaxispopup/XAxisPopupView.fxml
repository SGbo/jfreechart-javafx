   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox alignment="CENTER" prefHeight="29.0" prefWidth="50.0" GridPane.columnIndex="1">
         <children>
            <ChoiceBox fx:id="hourChoiceBox" prefHeight="26.0" prefWidth="80.0" />
            <Label text=" : " />
            <ChoiceBox fx:id="minuteChoiceBox" prefWidth="80.0" />
            <Label text=" : " />
            <ChoiceBox fx:id="secondChoiceBox" prefWidth="80.0" />
         </children>
      </HBox>
      <CheckBox mnemonicParsing="false" text="Live" GridPane.rowIndex="1" />
      <CheckBox mnemonicParsing="false" text="Show Everything" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
