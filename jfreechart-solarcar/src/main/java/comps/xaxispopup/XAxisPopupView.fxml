<GridPane hgap="10.0" prefHeight="79.0" prefWidth="362.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comps.xaxispopup.XAxisPopupController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" minWidth="10.0" prefWidth="151.0" />
